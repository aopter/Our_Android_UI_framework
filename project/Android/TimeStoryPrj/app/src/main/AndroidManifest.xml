<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.onest.timestoryprj">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activity.dynasty.DynastyIntroduceActivity"
            android:screenOrientation="landscape"></activity>
        <activity android:name=".activity.problem.ProblemInfoActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".activity.problem.SelectProblemTypeActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".dialog.dynasty.EventDialogActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".activity.dynasty.DetailsEventActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.dynasty.HomepageActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".activity.user.SettingActivity"
            android:screenOrientation="landscape"/>

        <activity
            android:name=".activity.user.RegisterActivity"
            android:screenOrientation="landscape" />

        <activity
            android:name=".activity.user.LoginActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.card.SpectficCardDetailActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.card.SpecificDynastyCardActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".activity.card.MyCardActivity" />
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>